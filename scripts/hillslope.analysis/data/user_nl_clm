fsurdat = '$CLM_USRDAT_DIR/surfdata_OSBS_hist_1850_78pfts_c251002.nc'
hillslope_file = '$CLM_USRDAT_DIR/hillslopes_osbs_c240416.nc'
use_init_interp = .false.
use_hillslope = .true.

! h1 stream: Column-level integrated carbon/water variables
hist_fincl2 = 'TOTECOSYSC', 'TOTECOSYSN', 'TOTSOMC', 'TOTSOMN', 'TOTVEGC', 'TOTVEGN', 'GPP', 'CPOOL', 'NPP', 'TWS', 'H2OSNO', 'ZWT'

! h2 stream: Gridcell-averaged vertically-resolved carbon/nitrogen pools and soil conditions
hist_fincl3 = 'ACT_SOMC_vr', 'CEL_LITC_vr', 'CPOOL', 'CWDC', 'CWDC_vr', 'CWDN', 'CWDN_vr', 'H2OSOI', 'TSOI', 'SOILC_vr', 'SOILN_vr', 'ZWT'

! Grid averaging control (true=gridcell averaged, false=vector format)
hist_dov2xy = .true., .false., .true.

! Subgrid level for vector format (only applies when dov2xy=false)
hist_type1d_pertape = ' ', 'COLS', ' '

! Output frequency (0=monthly, negative=hours, positive=timesteps)
hist_nhtfrq = 0, 0, 0

! Number of time samples per file (1=new file each month)
hist_mfilt = 1, 1, 1
